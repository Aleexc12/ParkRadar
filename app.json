{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyA7KcEErUn1gTp1rvrDwX0K3lrPoR7ZO5U"
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA7KcEErUn1gTp1rvrDwX0K3lrPoR7ZO5U"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "react-native-maps",
        {
          "enableIosGoogleMaps": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}